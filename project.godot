; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[animation]

warnings/check_invalid_track_paths=false

[application]

config/name="Barkley 2 (Godot port)"
config/description="Tales of Game's unfinished project recreated on Godot."
config/version="0.2.0"
run/main_scene="res://barkley2/rooms/r_title.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
boot_splash/show_image=false
boot_splash/use_filter=false
config/icon="res://barkley2/assets/icon/runner_icon.png"

[autoload]

FileSearch="*res://addons/file_search/search_dir.gd"
B2_CManager="*res://barkley2/autoloads/cinema_manager.gd"
B2_Shaders="*res://barkley2/autoloads/shaders.tscn"
B2_Config="*res://barkley2/autoloads/config.gd"
B2_Music="*res://barkley2/autoloads/music.tscn"
B2_Sound="*res://barkley2/autoloads/sound.tscn"
B2_RoomXY="*res://barkley2/autoloads/room_xy.tscn"
B2_Screen="*res://barkley2/autoloads/screen.tscn"
B2_Playerdata="*res://barkley2/autoloads/playerdata.gd"
B2_Gamedata="*res://barkley2/autoloads/gamedata.gd"
B2_Input="*res://barkley2/autoloads/input_manager.gd"
B2_Map="*res://barkley2/autoloads/map.gd"
B2_Note="*res://barkley2/autoloads/note.gd"
B2_Debug="*res://barkley2/resources/B2_Debug/b2_debug.tscn"

[debug]

file_logging/enable_file_logging=true
file_logging/log_path="user://logs/b2_debug.log"
gdscript/warnings/unused_variable=0
gdscript/warnings/unused_private_class_variable=0
gdscript/warnings/unused_signal=0
shapes/navigation/geometry_edge_color=Color(0.5, 1, 1, 0.416)
shapes/navigation/geometry_face_color=Color(0.5, 1, 1, 0.195)
shapes/collision/shape_color=Color(0, 0.6, 0.7, 0.254)

[display]

window/size/viewport_width=384
window/size/viewport_height=240
window/size/window_width_override=768
window/size/window_height_override=480
window/subwindows/embed_subwindows=false
window/stretch/mode="canvas_items"
window/stretch/scale_mode="integer"
window/vsync/vsync_mode=2

[editor]

naming/scene_name_casing=0

[editor_plugins]

enabled=PackedStringArray("res://addons/file_search/plugin.cfg", "res://addons/texture_fonts/plugin.cfg")

[file_customization]

folder_colors={
"res://barkley2/scenes/": "green",
"res://barkley2/scenes/Objects/_enemies/": "yellow",
"res://tools/": "blue"
}

[global_group]

navigation_polygon_source_geometry_group=""
ped_waypoint=""
ped_exit=""
ped_entrance=""

[importer_defaults]

font_data_image={
"character_margin": Rect2i(0, 0, 0, 0),
"character_ranges": PackedStringArray("32-126"),
"columns": 12,
"compress": false,
"fallbacks": [],
"image_margin": Rect2i(0, 0, 0, 0),
"rows": 9,
"scaling_mode": 1
}
wav={
"compress/mode": 1
}

