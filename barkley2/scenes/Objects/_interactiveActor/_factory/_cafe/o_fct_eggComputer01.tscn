[sub_resource type="GDScript" id="GDScript_lptff"]
script/source = "extends B2_EnvironInteractive

@onready var s_fct_egg_computer_screen_01: AnimatedSprite2D 	= $s_fct_eggComputerScreen01
@onready var s_fct_egg_computer_side_01: AnimatedSprite2D 	= $s_fct_eggComputerSide01


func _ready() -> void:
	speed_scale = randf_range(0.9, 1.1)

func execute_event_user_0():
	## Noob detected.
	show_warning()

func show_warning():
	s_fct_egg_computer_screen_01.play(\"warning\")
	s_fct_egg_computer_side_01.frame = 1

func show_error():
	s_fct_egg_computer_screen_01.play(\"error\")
	s_fct_egg_computer_side_01.frame = 2
"

