[sub_resource type="GDScript" id="GDScript_hfynk"]
script/source = "extends AnimatedSprite2D

@onready var eggs := [ $s_fct_egg01, $s_fct_egg02, $s_fct_egg03, $s_fct_egg04, $s_fct_egg05, $s_fct_egg06 ]

# what a mess
# check o_fct_eggPit01

func _ready() -> void:
	for i in 6:
		var egg : AnimatedSprite2D = eggs[i]
		egg.frame 			= randi_range( 0, 3 )
		egg.speed_scale 	= randf_range(0.8, 1.2) - ( i / 10.0 )
		egg.play()

func jostle_eggs():
	for i in 6:
		var egg : AnimatedSprite2D = eggs[i]
		egg.speed_scale 	*= 2.0
"

