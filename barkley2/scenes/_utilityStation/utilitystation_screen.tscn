[sub_resource type="GDScript" id="GDScript_jigd8"]
resource_name = "res_graph"
script/source = "@tool
extends Panel
const MULTI	:= 15.0
@export var triangle_offset := 0.7
var TRIANGLE : PackedVector2Array = [ Vector2(0, 0) * MULTI, Vector2(triangle_offset, 1) * MULTI, Vector2(-triangle_offset, 1) * MULTI ]
const COLORS_BG	:= Color( Color.GRAY, 0.5 )
var COLORS	: PackedColorArray = [
	Color( B2_Gamedata.c_cosmic, 	0.5 ),
	Color( B2_Gamedata.c_mental, 	0.5 ),
	Color( B2_Gamedata.c_cyber, 	0.5 ),
	Color( B2_Gamedata.c_bio, 		0.5 ),
	Color( B2_Gamedata.c_zauber, 	0.5 ),
	]
var stats := [
	B2_HoopzStats.STAT_BASE_RESISTANCE_COSMIC,
	B2_HoopzStats.STAT_BASE_RESISTANCE_MENTAL,
	B2_HoopzStats.STAT_BASE_RESISTANCE_CYBER,
	B2_HoopzStats.STAT_BASE_RESISTANCE_BIO,
	B2_HoopzStats.STAT_BASE_RESISTANCE_ZAUBER,
]
@export var rot_offset := 2.6

func _draw() -> void:
	for i in 5:
		#TRIANGLE = [ Vector2(0, 0) * MULTI, Vector2(triangle_offset, 1) * MULTI, Vector2(-triangle_offset, 1) * MULTI ]
		draw_set_transform( Vector2(size / 2).round(), ( ( PI / rot_offset ) * float(i) ) )
		draw_colored_polygon( TRIANGLE, COLORS_BG )
		draw_colored_polygon( PackedVector2Array( [
			TRIANGLE[0] * 0.5 * ( B2_Playerdata.player_stats.get_effective_stat( stats[i] ) + 100 ) / 100,
			TRIANGLE[1] * 0.5 * ( B2_Playerdata.player_stats.get_effective_stat( stats[i] ) + 100 ) / 100,
			TRIANGLE[2] * 0.5 * ( B2_Playerdata.player_stats.get_effective_stat( stats[i] ) + 100 ) / 100,
			]), COLORS[i] )
		draw_polyline( TRIANGLE, Color.DIM_GRAY, 1.0 )
"

