; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Barkley2_Godot"
config/description="Tales of Game's unfinished project recreated on Godot."
run/main_scene="res://barkley2/rooms/r_title.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
run/max_fps=30
boot_splash/image="res://barkley2/assets/splash/s_toglogo_0.png"
config/icon="res://barkley2/assets/icon/runner_icon.png"

[autoload]

B2_GameInit="*res://barkley2/autoloads/game_init.gd"
B2_Settings="*res://barkley2/autoloads/settings.gd"
B2_Balance="*res://barkley2/autoloads/balance.gd"
B2_Config="*res://barkley2/autoloads/config.gd"
B2_Music="*res://barkley2/autoloads/music.tscn"
B2_Sound="*res://barkley2/autoloads/sound.tscn"
B2_Border="*res://barkley2/autoloads/border.gd"

[debug]

gdscript/warnings/unused_variable=0

[display]

window/size/viewport_width=384
window/size/viewport_height=240
window/stretch/mode="viewport"
window/stretch/scale_mode="integer"
window/vsync/vsync_mode=0

[editor_plugins]

enabled=PackedStringArray("res://addons/texture_fonts/plugin.cfg")

[importer_defaults]

font_data_image={
"character_margin": Rect2i(0, 0, 0, 0),
"character_ranges": PackedStringArray("32-126"),
"columns": 12,
"compress": false,
"fallbacks": [],
"image_margin": Rect2i(0, 0, 0, 0),
"rows": 9,
"scaling_mode": 1
}

[rendering]

textures/canvas_textures/default_texture_filter=0
